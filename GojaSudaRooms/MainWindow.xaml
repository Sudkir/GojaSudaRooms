<Window x:Class="GojaSudaRooms.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GojaSudaRooms"
        mc:Ignorable="d"
          Title="Room interior" MinHeight="450" MinWidth="800"
        Height="450" Width="800" Icon="Resources\logo.png" Background="#EEEEEE">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="300"/>
            <RowDefinition Height="150"/>
        </Grid.RowDefinitions>
        <Frame x:Name="frmMain" Grid.Row="0"/>
        <StackPanel Grid.Row="1" Orientation="Vertical">
            <TextBlock Text="Room interior" 
                       HorizontalAlignment="Center" 
                       VerticalAlignment="Bottom"/>
            <Image Source="Resources/logo.png" Width="50"/>
        </StackPanel>

    </Grid>
</Window>
